{
  "autoload": {
    "psr-4": {
      "Controller\\": "src/Controller",
      "Core\\": "src/Core",
      "Model\\": "src/Model",
      "View\\": "src/View"
    }
  },
  "require": {
    "texlab/mydb": "^0.0.6",
    "texlab/html": "^0.03.0",
    "ext-json": "*"
  },
  "scripts": {
    "idb": "php install/install.php",
    "rs": "php -S localhost:8000 -t public/",
    "psalm": "psalm",
    "phpstan": "phpstan analyse -c phpstan.neon",
    "codestyle": "phpcs",
    "codestylefix": "phpcbf"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.37",
    "vimeo/psalm": "^3.14",
    "phpunit/phpunit": "^9.3"
  }
}
